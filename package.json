    "babel-loader": "^8.1.0",
    "babel-plugin-macros": "^2.8.0",
    "cross-env": "^7.0.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "^16.13.1",
    "react-scripts": "3.4.3",
